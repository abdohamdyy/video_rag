# Rename this file to ".env" locally (do not commit your real key)
GEMINI_API_KEY=your_key_here

# Optional: choose a default model
GEMINI_MODEL=gemini-2.5-pro

# Optional: max video bytes to download/accept (default: 25MB)
MAX_VIDEO_BYTES=26214400

# ElevenLabs API keys
# Read key: for read operations (list, get documents)
ELEVENLABS_API_KEY=your_elevenlabs_read_key_here

# Write key: for write operations (upload, delete, sync documents)
# If not provided, read key will be used for both (backward compatibility)
ELEVENLABS_API_KEY_WRITE=your_elevenlabs_write_key_here

# ElevenLabs Agent and Knowledge Base settings
ELEVENLABS_AGENT_ID=your_agent_id_here
ELEVENLABS_KNOWLEDGE_BASE_ID=your_knowledge_base_id_here
DEFAULT_FOLLOWUP_MODE=troubleshooting
AUTO_SYNC_KB=false

